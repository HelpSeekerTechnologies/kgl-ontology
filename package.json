{
  "name": "@helpseeker/kgl-ontology",
  "version": "1.3.0",
  "description": "KGL (Kompas Glyph Language) v1.3 Ontology - A universal data modeling language with 45 canonical nodes, 7 modifiers, and 41 universal validation rules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "kgl",
    "kompas",
    "ontology",
    "data-model",
    "validation",
    "taxonomy",
    "semantic",
    "helpseeker"
  ],
  "author": "HelpSeeker Technologies",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HelpSeekerTechnologies/kgl-ontology.git"
  },
  "bugs": {
    "url": "https://github.com/HelpSeekerTechnologies/kgl-ontology/issues"
  },
  "homepage": "https://github.com/HelpSeekerTechnologies/kgl-ontology#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ontology": {
      "import": "./dist/ontology/mod.js",
      "require": "./dist/ontology/mod.js",
      "types": "./dist/ontology/mod.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    }
  },
  "dependencies": {
    "neo4j-driver": "^6.0.1"
  }
}
